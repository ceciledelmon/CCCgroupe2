<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
    id          = "com.phonegap.example"
    versionCode = "10"
    version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Le Coon</name>

    <description>
      En attente de la description.
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
      Groupe 2
    </author>

	<plugin name="cordova-plugin-camera" spec="1.2.0" source="pgb" />
    <plugin name="cordova-plugin-file" spec="3.0.0" source="pgb" />
    <plugin name="cordova-plugin-file-transfer" spec="1.3.0" source="pgb" />
    <plugin name="cordova-plugin-whitelist" />

	<feature name="http://api.phonegap.com/1.0/network" />
	
	<preference name="KeepRunning" value="true" /> 
    <preference name="exit-on-suspend" value="false" />

	<!-- Autorisation de navigation sur Internet -->
	<access origin="*" />
	<allow-intent href="*" />
	<allow-navigation href="*" />


</widget>
